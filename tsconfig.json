{
	"compilerOptions": {
		"target": "ES6",
        //"module": "ES6",
        "lib": ["ES6", "DOM"],
        "outDir": "out",
        "rootDir": "src",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "moduleResolution": "node",
        "jsx": "react-jsx",
        "module": "ESNext"
    },
    "include": ["src"],
    "exclude": ["node_modules", ".vscode-test"]
}
